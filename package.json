{
  "name": "kids-app",
  "version": "1.0.0",
  "description": "A preschool learning web application",
  "type": "module",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./",
    "build-app": "parcel build src/index.html --public-url ./ && echo > dist/.nojekyll",
    "deploy": "gh-pages -d dist",
    "deploy-app": "gh-pages -d dist && git add . && git commit -m 'Deploy updates to GitHub Pages' && git push",
    "clean": "rm -rf .parcel-cache dist node_modules",
    "startLogs": "parcel src/index.html --log-level verbose",
    "test": "jest"
  },
  "keywords": [
    "preschool",
    "education",
    "learning",
    "kids"
  ],
  "author": "Johanna Calderon",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^4.1.0",
    "jsdom": "^26.0.0",
    "parcel": "^2.13.3",
    "svgo": "^3.3.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
